{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
        "arrowFunctions": true,
        "binaryLiterals": true,
        "blockBindings": true,
        "classes": true,
        "defaultParams": true,
        "destructuring": true,
        "forOf": true,
        "generators": true,
        "modules": true,
        "objectLiteralComputedProperties": true,
        "objectLiteralDuplicateProperties": true,
        "objectLiteralShorthandMethods": true,
        "objectLiteralShorthandProperties": true,
        "octalLiterals": true,
        "regexUFlag": true,
        "regexYFlag": true,
        "spread": true,
        "superInFunctions": true,
        "templateStrings": true,
        "unicodeCodePointEscapes": true,
        "globalReturn": true,
        "jsx": true,
        "experimentalObjectRestSpread": true
    }
  },
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true,
    "mocha": true,
    "es6": true
  },
  "globals": {
    "expect": false,
    "curry": true,
    "path": true,
    "filter": true,
    "counter": true,
    "count": true,
    "find": true,
    "select": true,
    "pipe": true,
    "exclude": true,
    "map": true,
    "mix": true,
    "margin": true,
    "marginTop": true,
    "marginRight": true,
    "marginBottom": true,
    "marginLeft": true,
    "absolute": true,
    "padding": true,
    "paddingTop": true,
    "paddingRight": true,
    "paddingBottom": true,
    "paddingLeft": true,
    "border": true,
    "borderRadius": true,
    "borderTopLeftRadius": true,
    "borderTopRightRadius": true,
    "borderBottomLeftRadius": true,
    "borderBottomRightRadius": true,
    "borderTop": true,
    "borderRight": true,
    "borderBottom": true,
    "borderLeft": true,
    "background": true,
    "fontSize": true,
    "fontColor": true,
    "fontWeight": true,
    "flexContainer": true,
    "flexVertical": true,
    "flexHorizontal": true,
    "flexItem": true,
    "flexGrow": true,
    "alignSelf": true,
    "Box": true,
    "BaseText": true,
    "Text": true,
    "LabelLight": true,
    "Button": true,
    "TextButton": true,
    "Image": true,
    "ScrollView": true,
    "FixedPage": true,
    "Header": true,
    "Footer": true,
    "React": true,
    "Component": true,
    "PropTypes": true,
    "vw": true,
    "vh": true,
    "rem": true,
    "defaultTo": true,
    "log": true,
    "mapAsArray": true,
    "transport": true,
    "store": true,
    "messageLog": true,
    "initializeState": true,
    "width": true,
    "height": true,
    "minWidth": true,
    "minHeight": true,
    "maxWidth": true,
    "maxHeight": true,
    "lineHeight": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "indent": ["error", 2],
    "import/no-unresolved": [2, { "ignore": [".png$"] }],
    "react/require-extension": "off",
    "react/no-unused-prop-types": 0,
    "react/require-default-props": 1,
    "react/forbid-prop-types": [ "error", { "forbid": [ "any" ] } ],
    "no-nested-ternary": 1,
    "comma-dangle": 0,
    "react/jsx-equals-spacing": 0,
    "react/prefer-es6-class": 0,
    "react/wrap-multilines": 0,
    "no-unused-vars": 1,
    "no-multi-spaces": 1,
    "no-delete-var": 1,
    "no-shadow": 1,
    "no-restricted-globals": 1,
    "max-depth": ["error", 3],
    "max-statements": ["error", 8],
    "max-params": ["error", 3],
    "max-len": ["error", 90, 2, { "ignorePattern": "^import.*" } ],
    "jsx-a11y/tabindex-no-positive": 0,
    "semi": [
      "error",
      "never"
    ],
    "no-restricted-syntax": [
      "error",
      "WithStatement",
      "switch"
    ],
    "complexity": [
      "error",
      {
        "max": 5
      }
    ],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  },
  "settings": {
    "import/resolver": {
      "babel-module": {}
    }
  }
}